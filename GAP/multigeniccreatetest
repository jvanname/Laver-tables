x:=[1,1,[[1]]];

allextensions:=function(x)
local zero,one,table,n,exptable;

zero:=x[1];
one:=x[2];
table:=x[3];
n:=Length(table);

#exptable:=StructuralCopy(table);
#exptable[n+1]:=[1..n];
#exptable[1][n+1]:=n+1;
#for i in [2..n+1] do
#exptable[i][n+1]:=1;
#od;

mastertable:=[[]];
Add(mastertable[1],[]);
Add(mastertable[1],[[1..n]]);

for i in [2..n] do
mastertable[i]:=[[]];

for aa in mastertable[table[i][zero]] do
for bb in mastertable[table[i][one]] do

od;
od;


od;

end;
