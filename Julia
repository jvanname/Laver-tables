end

function multiextendtable(type, generators, table, extension)
    n = length(table)
    mix = []
    newtable = deepcopy(table)
    finalcritlist = critfind(table)
    gg = length(generators)
    push!(finalcritlist, n+1)
    mv = makefunction(table)
    newgenerators = []

    cardarray = [extension[v][type[v]] for v in 1:gg]
    imagearray = [cardarray[v][1] for v in 1:gg]
    tablearray = [cardarray[v][2] for v in 1:gg]
    pointarray = [cardarray[v][3] for v in 1:gg]
    mlist = [length(imagearray[v]) for v in 1:gg]
    sumlist = [n+1]
    for iz in 1:gg
        push!(sumlist,sumlist[iz] + mlist[iz] - 1);
    end

    imagespace = deepcopy(imagearray[1])
    for kz in 2:gg
        for is in 2:length(imagearray[kz])
            push!(imagespace, imagearray[kz][is])
        end
    end

    tablespace = deepcopy(tablearray[1])
    for kz in 2:gg
        for is in 2:length(tablearray[kz])
            quart = deepcopy(tablearray[kz][is])
            for js in 1:length(quart)
                if quart[js] > n+1
                    quart[js] = quart[js] + sumlist[kz] - n - 1
                end
            end
            push!(tablespace, quart)
        end
    end
    append!(newtable, tablespace)
# THERE IS AN ERROR HERE. I CANNOT PUSH.
    for a in finalcritlist
        if a > 1
            for b in imagespace
                push!(newtable[a], newtable[a][b])
            end
        end
    end
    append!(newtable[1], n+1:n+length(imagespace))

    for i in 1:gg
        if mlist[i] > 1
            push!(newgenerators,sumlist[i+1]);
        else
            if generators[i] > 1
                push!(newgenerators,generators[i]);
            else
                if pointarray[i] == 0
                    push!(newgenerators,n+1);
                elseif pointarray[i] == 1
                    push!(newgenerators,1);
                end
            end
        end
    end

    return [newgenerators, newtable]
end

function findimproper(generators, table, ext)
    n = length(table)
    improper = []

    for i in 1:length(ext)
        for j in 1:length(ext[i])
            imagecheck = ext[i][j][1]
            tablecheck = ext[i][j][2]
            pointcheck = ext[i][j][3]
            cont = false

            for iz in 2:length(tablecheck)
                if n+1 in tablecheck[iz]
                    cont = true
                end
            end

            if generators[i] == 1 && pointcheck == 0
                cont = true
            end

            if cont continue; end;

                push!(improper, j)
                break
            end
        end
    return Tuple(improper);
end

function allmultiextensions(generators, table)
    ext = multigeneration(generators, table)
    cart = collect(Iterators.product([1:length(v) for v in ext]...))
    improper = findimproper(generators, table, ext)
    output = []

    for tup in cart
        if tup == improper
            continue
        end
        push!(output, multiextendtable(tup, generators, table, ext))
    end

    return output
end

 function randomextension(generators,table)
              n=length(table);
              gg=length(generators);
              ext=multigeneration(generators,table);
              improper=collect(findimproper(generators,table,ext));
       tup=deepcopy(improper)
       while true
       tup=[rand(1:length(v)) for v in ext]
       if !(tup==improper) break; end;
       end;
       return [multiextendtable(tup,generators,table,ext),prod([length(v) for v in ext])];
       end;

function critmaxcong(table)
dic=Dict();
mvv=makefunction(table);
n=length(table);
prearray=[];
mm=0;
for i in 1:n
if length(table[i])==n 
push!(prearray,mm+1);
else
can=map(v->prearray[v],table[i]);
if haskey(dic,can);
axe=dic[can];
else axe=false; end;
if axe==false 
dic[can]=mm+1;
push!(prearray,mm+1);
else 
push!(prearray,axe);
end;
end;
mm=max(mm,prearray[i]);
end;

newdic=Dict();

finalarray=[];
mm=0;
for i in 1:n
can=map(v->prearray[mvv(v,i)],1:n)

if haskey(newdic,can) axe=newdic[can]
else axe=false; end;

if axe==false 
newdic[can]=mm+1;
push!(finalarray,mm+1);
else
push!(finalarray,axe);
end;
mm=max(mm,finalarray[i]);
end;
return finalarray;
end;

function nightmaretest(x)
critlist=critfind(x);
mvv=makefunction(x);
for i in 1:length(x)
for k in critlist
if mvv(mvv(i,i),k)==1 && !(mvv(i,k)==1) return false; end;
end;
end;
return true;
end;

function ssgextensions(generators,table)
output=[];
n=length(table);
gg=length(generators);
mvv=makefunction(table);
ext=multigeneration(generators,table);
improper=findimproper(generators,table,ext);
output=[];
for i in 1:length(ext)
push!(output,[]);
for j in 1:length(ext[i])
tuple=collect(deepcopy(improper));
if j==improper[i] continue; end;
tuple[i]=j;
proposal=multiextendtable(tuple,generators,table,ext);
if !nightmaretest(proposal[2]) continue; end;
push!(output[i],proposal);
end;
end;
return output;
end;

function makematrices(generators,table)
dic=Dict();
critlist=critfind(table);
cc=critlist[length(critlist)-1];
mvv=makefunction(table);
n=length(table);
prearray=zeros(Int64,cc);
prearray[cc]=1;
mm=1;
for i in cc+1:n
can=map(v->prearray[mvv(i,v)],generators);
if sum(can)==0 
push!(prearray,0); continue;
end;
if haskey(dic,can);
axe=dic[can];
else axe=false; end;
if axe==false 
dic[can]=mm+1;
push!(prearray,mm+1);
else 
push!(prearray,axe);
end;
mm=max(mm,prearray[i]);
end;
invarray=zeros(Int64,mm);
for i in 1:n
if prearray[i]>0
invarray[prearray[i]]=i;
end;
end;
A=[];
for g in generators
mat=spzeros(BigInt,mm,mm);
q=prearray[g];
if q>0
mat[1,q]+=1;
end;
for i in 2:mm
q=prearray[mvv(invarray[i],g)]
if q>0
mat[i,q]+=1;
end;
end;
push!(A,mat);
end;
return A;
end;

###################################

function position(n, list)
    m = length(list)
    if m == 0
        return 0
    end
    r = floor(Int, log2(m))
    p = 0
    if list[1] <= list[end]
        for i in reverse(0:r)
            if p + 2^i <= m
                if list[p + 2^i] <= n
                    p += 2^i
                end
            end
        end
        if p == 0
            return 0
        end
        return list[p] == n ? p : 0
    else
        for i in reverse(0:r)
            if p + 2^i <= m
                if list[p + 2^i] >= n
                    p += 2^i
                end
            end
        end
        if p == 0
            return 0
        end
        return list[p] == n ? p : 0
    end
end

function makefunction(compressedtable)
    n = length(compressedtable)
    tree = function(x, y)
        if length(compressedtable[x]) == n
            return compressedtable[x][y]
        else
            return compressedtable[x][compressedtable[end][y]]
        end
    end
    return tree
end

function megatest(power, limit, zero, one, table)
    return ultratest(power, limit, zero, one, makefunction(table), false)
end

function enlargetable(xxx, sequencemode)
    zero = xxx[1]
    one = xxx[2]
    table = xxx[3]
    n = length(table)
    m = length(sequencemode)

    if sequencemode[end] == 0
        xxx[1] = n + m
    else
        xxx[2] = n + m
    end

    tuple = ultratest(Inf, Inf, zero, one, makefunction(table), sequencemode)

    critlist = tuple[1]
    superlist = tuple[2]
    extender = tuple[3]
    extendcrit = tuple[4]
    extendlength = tuple[5]
    extendtable = tuple[6]

    # Extend table with new entry
    push!(table, vcat(1:n, extender))

    # Append to table[1]
    append!(table[1], (n+1):(n+m))

    for i in 1:(length(critlist)-1)
        for v in extender
            push!(table[critlist[i]], table[critlist[i]][v])
        end
    end

    for i in 2:m
        if extendtable[i][critlist[superlist[extender[i]]]] == 1
            table[n + i] = []
            for j in 1:(critlist[superlist[extender[i]]] - 1)
                if length(table) < n + i
                    push!(table, [])
                end
                if length(table[n + i]) < j
                    push!(table[n + i], extendtable[i][j])
                else
                    table[n + i][j] = extendtable[i][j]
                end
            end
        else
            if length(table) < n + i
                push!(table, [])
            end
            table[n + i] = extendtable[i]
        end
    end
end

function ultratest(power, limit, zero, one, tablefunction, sequencemode)
    beef = []
    
    n = max(zero, one)
    
    counter = 0
    
    critlist = []
    for i in 2:n
        if tablefunction(i, i) == 1
            push!(critlist, i)
        end
    end
    push!(critlist, 1)
    
    superlist = Vector{Int}(undef, n)
    for i in 1:n
        for j in 1:length(critlist)
            if tablefunction(i, critlist[j]) == 1
                superlist[i] = j
                break
            end
        end
    end
    
    orangezero = [Int[] for _ in 1:n]
    orangeone = [Int[] for _ in 1:n]
    
    for i in 2:n
        push!(orangezero[tablefunction(i, zero)], i)
        push!(orangeone[tablefunction(i, one)], i)
    end
    
    sequence = Int[]
    startpoint = 1
    
    extender = [[]]
    extendtable = [[]]
    extendcrit = [[]]
    extendlength = [[]]
    
    if isa(sequencemode, AbstractArray)
        pathsequence = reverse(sequencemode)
        if pathsequence[1] == 0
            pathextender = [zero]
        else
            pathextender = [one]
        end
        for i in 2:length(pathsequence)
            if pathsequence[i] == 0
                push!(pathextender, tablefunction(pathextender[i-1], zero))
            else
                push!(pathextender, tablefunction(pathextender[i-1], one))
            end
        end
        pathextender = reverse(pathextender)
    end
    
    function testfunction()
        counter += 1
        
        if isa(sequencemode, AbstractArray)
            if length(sequence) + 1 > length(pathextender)
                return false
            end
            if startpoint != pathextender[length(sequence) + 1]
                return false
            end
        end
        
        if length(sequence) >= power
            return false
        end
        
        while length(extender) > length(sequence)
            pop!(extender)
            pop!(extendtable)
            pop!(extendcrit)
            pop!(extendlength)
        end
        
        push!(extender, startpoint)
        
        if startpoint > 1
            iii = length(extender)
            
            push!(extendtable, Dict{Int, Int}())
            aa = critlist[1 + superlist[startpoint]]
            if aa > 1
                push!(extendlength, aa - 1)
                push!(extendcrit, aa)
            else
                push!(extendlength, n)
                push!(extendcrit, n + 1)
            end
            
            if sequence[iii-1] == 0
                key1 = superproduct(extendcrit[iii], zero)
                if haskey(extendtable[iii], key1)
                    if extendtable[iii][key1] != n + iii - 1
                        return false
                    end
                end
                key2 = superproduct(extendcrit[iii], one)
                if haskey(extendtable[iii], key2)
                    if extendtable[iii][key2] != tablefunction(extender[iii], one)
                        return false
                    end
                end
                extendtable[iii][key1] = n + iii - 1
                extendtable[iii][key2] = tablefunction(extender[iii], one)
            else
                key1 = superproduct(extendcrit[iii], zero)
                if haskey(extendtable[iii], key1)
                    if extendtable[iii][key1] != tablefunction(extender[iii], zero)
                        return false
                    end
                end
                key2 = superproduct(extendcrit[iii], one)
                if haskey(extendtable[iii], key2)
                    if extendtable[iii][key2] != n + iii - 1
                        return false
                    end
                end
                extendtable[iii][key1] = tablefunction(extender[iii], zero)
                extendtable[iii][key2] = n + iii - 1
            end
            
            for j in reverse(2:extendlength[iii])
                a = superproduct(superproduct(n + iii, j), superproduct(n + iii, zero))
                if a === false
                    return false
                end
                key_a = superproduct(j, zero)
                if haskey(extendtable[iii], key_a) && extendtable[iii][key_a] != a
                    return false
                end
                extendtable[iii][key_a] = a
                
                b = superproduct(superproduct(n + iii, j), superproduct(n + iii, one))
                if b === false
                    return false
                end
                key_b = superproduct(j, one)
                if haskey(extendtable[iii], key_b) && extendtable[iii][key_b] != b
                    return false
                end
                extendtable[iii][key_b] = b
            end
            
            if extendtable[iii][1] != 1
                return false
            end
        end
        
        return true
    end
    
    function superproduct(x, y)
        if x === false || y === false
            return false
        elseif x == 1
            return y
        elseif y > n
            return superproduct(x, extender[y - n])
        elseif x <= n
            return tablefunction(x, y)
        elseif x == n + 1
            return y
        else
            key = superproduct(extendcrit[x - n], y)
            if haskey(extendtable[x - n], key)
                return extendtable[x - n][key]
            else
                return false
            end
        end
    end
    
    output = []
    
    coin = true
    moo = zeros(Int, n + 1)
    
    while (length(sequence) > 0 || coin) && length(output) < limit
        push!(beef, deepcopy([sequence, extender, extendtable]))
        fart = get(Main, :fart, 0) + 1
        Main.fart = fart
        moo[length(sequence) + 1] += 1
        test = testfunction()
        
        push!(beef, test)
        
        if test && startpoint == zero
            push!(output, vcat(sequence, [0]))
        end
        if test && startpoint == one
            push!(output, vcat(sequence, [1]))
        end
        
        if length(output) > 0 && isa(sequencemode, AbstractArray)
            return [critlist, superlist, extender, extendcrit, extendlength, extendtable]
        end
        
        coin = false
        if length(orangezero[startpoint]) > 0 && test
            push!(sequence, 0)
            startpoint = orangezero[startpoint][1]
        elseif length(orangeone[startpoint]) > 0 && test
            push!(sequence, 1)
            startpoint = orangeone[startpoint][1]
        else
            while length(sequence) > 0
                cat = pop!(sequence)
                
                if cat == 0
                    newpoint = tablefunction(startpoint, zero)
                else
                    newpoint = tablefunction(startpoint, one)
                end
                
                if cat == 0
                    pos = findfirst(==(startpoint), orangezero[newpoint])
                else
                    pos = findfirst(==(startpoint), orangeone[newpoint])
                end
                
                if pos === nothing
                    pos = 0
                end
                
                if cat == 0 && pos < length(orangezero[newpoint])
                    startpoint = orangezero[newpoint][pos + 1]
                    push!(sequence, cat)
                    break
                end
                if cat == 1 && pos < length(orangeone[newpoint])
                    startpoint = orangeone[newpoint][pos + 1]
                    push!(sequence, cat)
                    break
                end
                if cat == 0 && pos == length(orangezero[newpoint]) && length(orangeone[newpoint]) > 0
                    startpoint = orangeone[newpoint][1]
                    push!(sequence, 1)
                    break
                end
                if (cat == 0 && pos == length(orangezero[newpoint]) && length(orangeone[newpoint]) == 0) || 
                   (cat == 1 && pos == length(orangeone[newpoint]))
                    startpoint = newpoint
                end
            end
        end
    end
    
    return output
end
